<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close All">
        <Item>
          <IdeaActionID>CloseAllEditors</IdeaActionID>
          <ShortCut>'⇧⌘W'</ShortCut>
          <Description>Close All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'⌥⇧⌘C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'code-challenge'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌘F11'</ShortCut>
          <Description>Debug 'code-challenge'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Module Settings">
        <Item>
          <IdeaActionID>OpenModuleSettings</IdeaActionID>
          <ShortCut>'F3'</ShortCut>
          <Description>Open Module Settings</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'⇧⌘F'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Test'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧⌘F11'</ShortCut>
          <Description>Run 'Test'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'All in code-challenge (3)'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'All in code-challenge (3)'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⇧⌘B'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>